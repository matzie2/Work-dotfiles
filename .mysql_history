use uk_emktg;
show tables;
desc rebates;
select * from rebates limit 1\G
select count(*) from rebates where expires>NOW();
select * from rebates where expires>NOW() LIMIT 10;
select * from rebates where expires>NOW();
select product_id,text from rebates where expires>NOW();
select * from rebates where product_id in ('NGEGA06GH7','NGEGA06GH8');
